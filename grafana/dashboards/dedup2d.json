{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Dedup2D Overview",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
      "collapsed": false
    },
    {
      "title": "Jobs Submitted (rate/min)",
      "type": "stat",
      "gridPos": {"h": 4, "w": 4, "x": 0, "y": 1},
      "targets": [
        {
          "expr": "sum(rate(dedup2d_jobs_submitted_total[5m])) * 60",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "red", "value": 100}
            ]
          }
        }
      }
    },
    {
      "title": "Success Rate",
      "type": "gauge",
      "gridPos": {"h": 4, "w": 4, "x": 4, "y": 1},
      "targets": [
        {
          "expr": "sum(rate(dedup2d_jobs_completed_total{status='success'}[5m])) / sum(rate(dedup2d_jobs_completed_total[5m])) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 90},
              {"color": "green", "value": 99}
            ]
          }
        }
      }
    },
    {
      "title": "Active Jobs",
      "type": "stat",
      "gridPos": {"h": 4, "w": 4, "x": 8, "y": 1},
      "targets": [
        {
          "expr": "sum(dedup2d_jobs_active)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 10}
            ]
          }
        }
      }
    },
    {
      "title": "Queued Jobs",
      "type": "stat",
      "gridPos": {"h": 4, "w": 4, "x": 12, "y": 1},
      "targets": [
        {
          "expr": "sum(dedup2d_jobs_queued)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 20},
              {"color": "red", "value": 100}
            ]
          }
        }
      }
    },
    {
      "title": "Error Rate EMA",
      "type": "stat",
      "gridPos": {"h": 4, "w": 4, "x": 16, "y": 1},
      "targets": [
        {
          "expr": "dedup2d_error_rate_ema * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 10}
            ]
          }
        }
      }
    },
    {
      "title": "P95 Job Duration",
      "type": "stat",
      "gridPos": {"h": 4, "w": 4, "x": 20, "y": 1},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(dedup2d_job_duration_seconds_bucket[5m])) by (le))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 30},
              {"color": "red", "value": 60}
            ]
          }
        }
      }
    },
    {
      "title": "Job Processing",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 5},
      "collapsed": false
    },
    {
      "title": "Jobs Submitted vs Completed",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
      "targets": [
        {
          "expr": "sum(rate(dedup2d_jobs_submitted_total[5m])) * 60",
          "legendFormat": "Submitted",
          "refId": "A"
        },
        {
          "expr": "sum(rate(dedup2d_jobs_completed_total{status='success'}[5m])) * 60",
          "legendFormat": "Completed (Success)",
          "refId": "B"
        },
        {
          "expr": "sum(rate(dedup2d_jobs_completed_total{status='error'}[5m])) * 60",
          "legendFormat": "Completed (Error)",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "title": "Job Duration Distribution",
      "type": "heatmap",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
      "targets": [
        {
          "expr": "sum(rate(dedup2d_job_duration_seconds_bucket[5m])) by (le)",
          "format": "heatmap",
          "legendFormat": "{{le}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "File Storage",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 14},
      "collapsed": false
    },
    {
      "title": "File Operations (rate/min)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 15},
      "targets": [
        {
          "expr": "sum(rate(dedup2d_file_uploads_total{status='success'}[5m])) by (backend) * 60",
          "legendFormat": "Upload ({{backend}})",
          "refId": "A"
        },
        {
          "expr": "sum(rate(dedup2d_file_downloads_total{status='success'}[5m])) by (backend) * 60",
          "legendFormat": "Download ({{backend}})",
          "refId": "B"
        },
        {
          "expr": "sum(rate(dedup2d_file_deletes_total{status='success'}[5m])) by (backend) * 60",
          "legendFormat": "Delete ({{backend}})",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "title": "File Size Distribution",
      "type": "heatmap",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 15},
      "targets": [
        {
          "expr": "sum(rate(dedup2d_file_upload_bytes_bucket[5m])) by (le)",
          "format": "heatmap",
          "legendFormat": "{{le}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Callbacks",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 23},
      "collapsed": false
    },
    {
      "title": "Callback Status",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
      "targets": [
        {
          "expr": "sum(rate(dedup2d_callbacks_total{status='success'}[5m])) * 60",
          "legendFormat": "Success",
          "refId": "A"
        },
        {
          "expr": "sum(rate(dedup2d_callbacks_total{status='error'}[5m])) * 60",
          "legendFormat": "Error",
          "refId": "B"
        },
        {
          "expr": "sum(rate(dedup2d_callbacks_total{status='blocked'}[5m])) * 60",
          "legendFormat": "Blocked",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "title": "Callback Latency (P95)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(dedup2d_callback_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(dedup2d_callback_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P50",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "GC & Maintenance",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 32},
      "collapsed": false
    },
    {
      "title": "GC Runs",
      "type": "timeseries",
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 33},
      "targets": [
        {
          "expr": "sum(increase(dedup2d_gc_runs_total{status='success'}[1h]))",
          "legendFormat": "Success",
          "refId": "A"
        },
        {
          "expr": "sum(increase(dedup2d_gc_runs_total{status='error'}[1h]))",
          "legendFormat": "Error",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Files Deleted by GC",
      "type": "timeseries",
      "gridPos": {"h": 6, "w": 8, "x": 8, "y": 33},
      "targets": [
        {
          "expr": "sum(increase(dedup2d_gc_files_deleted_total[1h])) by (backend)",
          "legendFormat": "{{backend}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Bytes Freed by GC",
      "type": "timeseries",
      "gridPos": {"h": 6, "w": 8, "x": 16, "y": 33},
      "targets": [
        {
          "expr": "sum(increase(dedup2d_gc_bytes_freed_total[1h])) by (backend)",
          "legendFormat": "{{backend}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    }
  ],
